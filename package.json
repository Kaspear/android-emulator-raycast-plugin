{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "android-emulator",
  "title": "Android emulator",
  "description": "Display avds and open simulator",
  "icon": "command-icon.png",
  "author": "Kaspear",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "emulator",
      "title": "Emulator",
      "subtitle": "Android emulators",
      "description": "Open and search available emulators",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.1",
    "exec": "^0.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.8",
    "react-devtools": "^4.24.6",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
